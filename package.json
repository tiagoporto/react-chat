{
  "name": "react-chat",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://tiagoporto.github.io/react-chat",
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=24",
      "onFail": "error"
    },
    "packageManager": {
      "name": "yarn",
      "version": ">=4",
      "onFail": "error"
    }
  },
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "check-updates": "yarn upgrade-interactive",
    "predeploy": "npm run build",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "dependencies": {
    "autoprefixer": "10.4.22",
    "babel-core": "6.26.3",
    "babel-loader": "10.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-react-app": "^10.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "chalk": "5.6.2",
    "css-loader": "7.1.2",
    "dotenv": "17.2.3",
    "dotenv-expand": "12.0.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "6.2.0",
    "fs-extra": "11.3.2",
    "html-webpack-plugin": "5.6.5",
    "i18n-react": "^0.7.0",
    "mobx": "^6.15.0",
    "mobx-react": "^9.2.1",
    "normalize.css": "^8.0.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "8.2.0",
    "promise": "8.3.0",
    "raf": "3.4.1",
    "react": "^19.2.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^19.2.0",
    "react-ga": "^3.3.1",
    "react-router-dom": "^7.10.0",
    "resolve": "1.22.11",
    "socket.io": "^4.8.1",
    "style-loader": "4.0.0",
    "stylus": "^0.64.0",
    "stylus-loader": "^8.1.2",
    "sw-precache-webpack-plugin": "1.0.0",
    "url-loader": "4.1.1",
    "webpack": "5.103.0",
    "webpack-dev-server": "5.2.2",
    "webpack-manifest-plugin": "6.0.1",
    "whatwg-fetch": "3.6.20"
  },
  "babel": {
    "plugins": [
      "transform-decorators-legacy"
    ],
    "presets": [
      "react-app"
    ]
  }
}
